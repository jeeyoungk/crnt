{
  "name": "crnt",
  "version": "0.0.20250809",
  "description": "TypeScript library for async concurrency primitives",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "tsup": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --write . --cache",
    "format:check": "prettier --check . --cache",
    "check": "bun run lint && bun run format:check && bun run typecheck"
  },
  "keywords": [
    "async",
    "concurrency",
    "primitives",
    "typescript"
  ],
  "author": "jeeyoungk",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  }
}
