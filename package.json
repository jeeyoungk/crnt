{
  "name": "crnt",
  "description": "TypeScript library for async concurrency primitives",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "tsup": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --write . --cache",
    "format:check": "prettier --check . --cache",
    "check": "bun run lint && bun run format:check && bun run typecheck",
    "docs": "typedoc",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "async",
    "concurrency",
    "primitives",
    "typescript"
  ],
  "author": "jeeyoungk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeeyoungk/crnt.git"
  },
  "homepage": "https://github.com/jeeyoungk/crnt",
  "bugs": {
    "url": "https://github.com/jeeyoungk/crnt/issues"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "provenance": true
  },
  "version": "0.0.8"
}
